produtoSelecionado.imagem} alt= {this.props.produtoSelecionado.descricao} />\n\n                        <div className=\"row justify-content-center\">\n                            <div className=\"col-sm-8 col-md-9 col-lg-9\">\n                                <form onSubmit={(evento)=> this.postOrder(evento)}>\n                                    <div className=\"form-group\">\n                                        <label htmlFor=\"nomeproduto\">Nome do produto</label> <br />\n                                        <input className=\"form-control\" type=\"text\" name=\"nomeproduto\" required defaultValue={this.props.produtoSelecionado.descricao} readOnly />\n                                        <br /><br />\n                                        <label htmlFor=\"valorunitario\">Preço</label> <br />\n                                        <input className=\"form-control\" type=\"number\" name=\"valorunitario\" required defaultValue={this.props.produtoSelecionado.precoVenda} readOnly />\n                                        <br /><br />\n                                        <label htmlFor=\"quantidade\">Quantidade</label> <br />\n                                        <input className=\"form-control\" type=\"number\" name=\"quantidade\" required defaultValue=\"1\" /> <br /><br />\n                                        <label htmlFor=\"nomecliente\">Nome</label> <br />\n                                        <input className=\"form-control\" type=\"text\" name=\"nomecliente\" required /> <br /><br />\n                                        <label htmlFor=\"email\">Email</label> <br />\n                                        <input className=\"form-control\" type=\"text\" name=\"email\" required /> <br /><br />\n                                        <label htmlFor=\"endereco\">Endereço</label> <br />\n                                        <input className=\"form-control\" type=\"text\" name=\"endereco\" required /> <br /><br />\n                                        <label htmlFor=\"telefone\">Telefone</label> <br />\n                                        <input className=\"form-control\" type=\"tel\" name=\"telefone\" required /> <br /><br /><br />\n                                        <button type=\"submit\" className=\"comprar-btn btn-danger\">Comprar</button>\n                                    </div>\n                                </form>\n                            </div>\n                        </div>\n                    </div>\n                }\n            </div>\n        )\n    }\n}\n\nconst mapStateToProps = store => ({\n    produtoSelecionado: store.prodSelecionadoState.produtoSelecionado\n})\n\nconst mapDispatchToProps = dispatch => bindActionCreators({ selecionarProduto }, dispatch)\n\nexport default connect(mapStateToProps, mapDispatchToProps)(withRouter(Pedido));","C:\\Users\\Usuário\\Documents\\Projetos\\Full-Stack-Eletro\\frontend\\src\\paginas\\ConfirmaPedido.jsx",[],"C:\\Users\\Usuário\\Documents\\Projetos\\Full-Stack-Eletro\\frontend\\src\\components\\Produto.jsx",[],"C:\\Users\\Usuário\\Documents\\Projetos\\Full-Stack-Eletro\\frontend\\src\\components\\Imagem.jsx",[],"C:\\Users\\Usuário\\Documents\\Projetos\\Full-Stack-Eletro\\frontend\\src\\store\\actions\\produtoSelecionadoActions.js",[],"C:\\Users\\Usuário\\Documents\\Projetos\\Full-Stack-Eletro\\frontend\\src\\store\\actions\\categoriasActions.js",[],"C:\\Users\\Usuário\\Documents\\Projetos\\Full-Stack-Eletro\\frontend\\src\\store\\index.js",[],"C:\\Users\\Usuário\\Documents\\Projetos\\Full-Stack-Eletro\\frontend\\src\\store\\reducers\\categoriasReducer.js",[],"C:\\Users\\Usuário\\Documents\\Projetos\\Full-Stack-Eletro\\frontend\\src\\store\\reducers\\produtoSelecionadoReducer.js",[],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":9,"column":5,"nodeType":"81","messageId":"82","endLine":11,"endColumn":6},"no-native-reassign",["83"],"no-negated-in-lh